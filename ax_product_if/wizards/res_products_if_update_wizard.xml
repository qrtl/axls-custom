<odoo>

    <record model="ir.ui.view" id="res_products_if_update_wizard_form_view">
        <field name="name">res.products.if.update.wizard.form (in ax_product_if)</field>
        <field name="model">res.products.if.update.wizard</field>
        <field name="arch" type="xml">
            <form string="Res Products If Update Wizard">
                <group name="filter">
                    <group name="date_range" colspan="2">
                        <group>
                            <field name="date_from" />
                        </group>
                        <group>
                            <field name="date_to" />
                        </group>
                    </group>
                </group>
                <footer>
                    <button name="action_update" string="Update" type="object" default_focus="1" class="oe_highlight" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
